{{addStylesheet "stylesheets/resume.css"}}

    <div id="resume">

    {{#basics}}
    <h1>{{name}}</h1>
    <h2>{{label}}</h2>
    <section id="basics">

        <div class="contact">
        {{#if email}}
        <span class="email">
            <a href="mailto:{{email}}">{{email}}</a>
        </span>
        {{/if}}
        {{#if phone}}
        <span class="phone">
            &middot; {{phone}}
        </span>
        {{/if}}
        {{#if website}}
        <span class="website">
            &middot; <a href="{{website}}">{{website}}</a>
        </span>
        {{/if}}
        </div>

        {{#location}}
        {{#if city}}
        <div class="city">
            {{city}}{{#if region}}, {{region}}{{/if}}
        </div>
        {{/if}}
        {{/location}}
        {{#if summary}}
        <div class="summary">
            <p>{{summary}}</p>
        </div>
        {{/if}}
        {{#if profiles.length}}
        <section id="profiles">
            {{#each profiles}}
            <span class="item">
                {{#if network}}
                {{#if url}}{{#if @first}}{{else}}&middot;{{/if}} <a href="{{url}}" class="network">{{/if}}{{network}}{{#if url}}</a>{{/if}}
                {{/if}}
            </span>
            {{/each}}
        </section>
        {{/if}}
    </section>
    {{/basics}}

    {{#if work.length}}
    <section id="work">
        <h2>Work</h2>
        {{#each work}}
        <div class="item">
            {{#if position}}
            <h3 class="position">
                {{position}}
            </h3>
            {{/if}}
            {{#if company}}
            <div class="name-date">
                {{#if website}}<a href="{{website}}">{{/if}}{{company}}{{#if website}}</a>{{/if}}
                &middot;
                {{#if startDate}}
                <span class="startDate">
                    {{startDate}}
                </span>
                {{/if}}
                {{#if endDate}}
                <span class="endDate">
                    - {{endDate}}
                </span>
                {{else}}
                <span class="endDate">
                    - Present
                </span>
                {{/if}} 
            </div>
            {{/if}}
            {{#if summary}}
            <div class="summary">
                <p>{{summary}}</p>
            </div>
            {{/if}}
            {{#if highlights.length}}
            <ul class="highlights">
                {{#each highlights}}
                <li>{{.}}</li>
                {{/each}}
            </ul>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if volunteer.length}}
    <section id="volunteer">
        <h2>Volunteer</h2>
        {{#each volunteer}}
        <div class="item">
            {{#if organization}}
            <h3 class="company">
                {{organization}}
            </h3>
            {{/if}}
            <div class="date">
                {{#if startDate}}
                <span class="startDate">
                    {{startDate}}
                </span>
                {{/if}}
                {{#if endDate}}
                <span class="endDate">
                    - {{endDate}}
                </span>
                {{else}}
                <span class="endDate">
                    - Present
                </span>
                {{/if}}
            </div>
            {{#if position}}
            <div class="position">
                {{position}}
            </div>
            {{/if}}
            {{#if website}}
            <div class="website">
                <a href="{{website}}">{{website}}</a>
            </div>
            {{/if}}
            {{#if summary}}
            <div class="summary">
                <p>{{summary}}</p>
            </div>
            {{/if}}
            {{#if highlights.length}}
            <ul class="highlights">
                {{#each highlights}}
                <li>{{.}}</li>
                {{/each}}
            </ul>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if education.length}}
    <section id="education">
        <h2>Education</h2>
        {{#each education}}
        <div class="item">
            {{#if institution}}
            <div class="institution">
                {{institution}}
            </div>
            {{/if}}
            {{#if area}}
            <div class="area">
                {{#if studyType}}{{studyType}}, {{/if}}{{area}}
            </div>
            {{/if}}
            {{#if gpa}}
            <div class="gpa">
                GPA: {{gpa}}
            </div>
            {{/if}}
            {{#if courses.length}}
            <ul class="courses">
                {{#each courses}}
                <li>{{.}}</li>
                {{/each}}
            </ul>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if awards.length}}
    <section id="awards">
        <h2>Awards</h2>
        {{#each awards}}
        <div class="item">
            {{#if title}}
            <div class="title">
                {{title}}
            </div>
            {{/if}}
            {{#if date}}
            <div class="date">
                {{date}}
            </div>
            {{/if}}
            {{#if awarder}}
            <div class="awarder">
                {{awarder}}
            </div>
            {{/if}}
            {{#if summary}}
            <div class="summary">
                <p>{{summary}}</p>
            </div>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if publications.length}}
    <section id="publications">
        <h2>Publications</h2>
        {{#each publications}}
        <div class="item">
            {{#if name}}
            <div class="name">
                {{name}}
            </div>
            {{/if}}
            {{#if publisher}}
            <div class="publisher">
                {{publisher}}
            </div>
            {{/if}}
            {{#if releaseDate}}
            <div class="releaseDate">
                {{releaseDate}}
            </div>
            {{/if}}
            {{#if website}}
            <div class="website">
                <a href="{{website}}">Link</a>
            </div>
            {{/if}}
            {{#if summary}}
            <div class="summary">
                <p>{{summary}}</p>
            </div>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if skills.length}}
    <section id="skills">
        <h2>Skills</h2>
        {{#each skills}}
        <div class="item">
            {{#if name}}
            <div class="name">
                {{name}}
            </div>
            {{/if}}
            {{#if level}}
            <div class="level">
                <em>{{level}}</em>
            </div>
            {{/if}}
            {{#if keywords.length}}
            <ul class="keywords">
                {{#each keywords}}
                <li>{{.}}</li>
                {{/each}}
            </ul>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if languages.length}}
    <section id="languages">
        <h2>Languages</h2>
        {{#each languages}}
        <div class="item">
            {{#if language}}
            <div class="language">
                {{language}}
            </div>
            {{/if}}
            {{#if fluency}}
            <div class="fluency">
                <em>{{fluency}}</em>
            </div>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if interests.length}}
    <section id="interests">
        <h2>Interests</h2>
        {{#each interests}}
        <div class="item">
            {{#if name}}
            <div class="name">
                {{name}}
            </div>
            {{/if}}
            {{#if keywords.length}}
            <ul class="keywords">
                {{#each keywords}}
                <li>{{.}}</li>
                {{/each}}
            </ul>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    {{#if references.length}}
    <section id="references">
        <h2>References</h2>
        {{#each references}}
        <div class="item">
            {{#if reference}}
            <blockquote class="reference">
                {{reference}}
            </blockquote>
            {{/if}}
            {{#if name}}
            <div class="name">
                â€” {{name}}
            </div>
            {{/if}}
        </div>
        {{/each}}
    </section>
    {{/if}}

    </div>
